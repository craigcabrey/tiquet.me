{
  "github-login": {
    "provider": "github",
    "module": "passport-github",
    "clientID": "{facebook-client-id-1}",
    "clientSecret": "{facebook-client-secret-1}",
    "callbackURL": "/auth/github/callback",
    "authPath": "/auth/github",
    "callbackPath": "/auth/github/callback",
    "successRedirect": "/",
    "failureRedirect": "/",
    "scope": ["email"],
    "failureFlash": true
  }
}
